{
    "name": "devsfort/fortblog",
    "version": "1.0.1",
    "description": "Fort Blog Publishing Framework.",
    "keywords": ["framework", "laravel", "blog","fort","publishing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Haseeb Ahmad",
            "email": "hahmad748@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3||^7.4||^8.0||^8.1||^8.2",
        "laravel/framework": "^7.0||^8.0||^9.0||^10.0||^10.1"
    },
    "autoload": {
        "psr-4": {
            "Fortblog\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Fortblog\\FortblogServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
